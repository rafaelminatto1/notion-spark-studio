    "analyze": "npx @next/bundle-analyzer",
    "clean": "rm -rf .next out coverage .vercel",
    "verify-env": "node scripts/verify-env.js",
    "system-status": "node scripts/system-status.js",
    "health-check": "npm run system-status && npm run verify-env && npm run lint:hybrid",
